{
  "name": "@chevrotain/types",
  "version": "1.0.0",
  "description": "reproduce bug in typedoc - only public methods are documented",
  "scripts": {
    "clean": "shx rm -rf dev lib",
    "compile:watch": "tsc -w",
    "compile": "tsc",
    "api-site:build": "typedoc api.d.ts --out dev/docs"
  },
  "devDependencies": {
    "typedoc": "0.23.19",
    "typescript": "4.7.4",
    "shx": "0.3.4"
  },
  "typings": "api.d.ts",
  "publishConfig": {
    "access": "public"
  }
}
